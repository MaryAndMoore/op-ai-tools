generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model ForumPost {
  id        Int      @id @default(autoincrement())
  url       String   @unique
  category  String
  about     String
  firstPost String
  reaction  String
  overview  String
  tldr      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
